<%=simple_form_for @account, url: :accounting_accounts do |f| %>
  <%= f.error_notification %>
  <%= f.input :name %>
  <%= f.input :code %>
  <%= f.input :type, collection: Accounting::Account.types %>
  <%= f.input :contra %>
  <%= f.submit %>
<% end %>
<%=link_to "Cancel", accounting_accounts_path %>
